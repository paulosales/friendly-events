    "@typescript-eslint/parser": "^2.20.0",
    "bootstrap3": "^3.3.5",
    "detect-browser-language": "^0.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^3.1.0",
    "infinite-react-carousel": "^1.2.9",
    "jquery": "^3.4.1",
    "leaflet": "^1.6.0",
